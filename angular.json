{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ion-intl-tel": {
      "projectType": "library",
      "root": "projects/ion-intl-tel",
      "sourceRoot": "projects/ion-intl-tel/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ion-intl-tel/tsconfig.lib.json",
            "project": "projects/ion-intl-tel/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ion-intl-tel/src/test.ts",
            "tsConfig": "projects/ion-intl-tel/tsconfig.spec.json",
            "karmaConfig": "projects/ion-intl-tel/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ion-intl-tel/tsconfig.lib.json",
              "projects/ion-intl-tel/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@ionic/angular-toolkit"
    ],
    "analytics": false
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    }
  }
}
